<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_tag login_path do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %>
  <%= label_tag :password %>
  <%= link_to "(forgot password", new_password_resets_path %>
  <%= password_field_tag :password %>
  <%= submit_tag "login" %>
<% end %>
